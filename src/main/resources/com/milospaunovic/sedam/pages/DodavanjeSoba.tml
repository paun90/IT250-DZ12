<html t:type="layout" title="Dodavanje Sedam"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

      <t:beaneditform object="soba" exclude="id" >
      </t:beaneditform>
      <t:datecomp/>
      <t:grid source="listaSoba" exclude="id" add="edit,delete" row="onesoba" >

        <p:editCell>
            <t:actionlink t:id="edit" context="onesoba">Edit</t:actionlink>
        </p:editCell>
        <p:deleteCell>
            <t:actionlink t:id="delete" context="onesoba.id">Delete</t:actionlink>
        </p:deleteCell>
        <p:imeSobeCell>
            <div t:id="imeSobe" value="soba.imeSobe" t:type="jquery/inPlaceEditor" t:inplace="true" t:context="onesoba.id"/>
        </p:imeSobeCell>
      </t:grid>


</html>
